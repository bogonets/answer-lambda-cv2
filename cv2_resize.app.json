{
    "info": {
        "name": "cv2_resize",
        "version": "1.0.0",
        "category": "cv2",
        "keywords": ["cv2"],
        "homepage": "https://answer.bogonets.com/",
        "bugs": "https://answer.bogonets.com/",
        "license": "Bogonet License",
        "author": "zer0",
        "dependencies": [
            {"type": "pip", "src": "numpy"},
            {"type": "pip", "src": "opencv-python"}
        ],
        "engines": ">=1.0.2",
        "environment": {
            "type": "pyenv",
            "name": "cv2"
        },
        "titles": {
            "en": "cv2.resize",
            "ko": "cv2.resize"
        },
        "descriptions": {
            "en": "Resizes an image.",
            "ko": "이미지 크기를 조정한다."
        },
        "documentation_mime": "text",
        "documentations": {},
        "meta": {}
    },
    "controls": {
        "input": "source",
        "output": "result"
    },
    "props": [
        {
            "rule": "read_and_write",
            "name": "width",
            "default_value": 0,
            "type": "int",
            "required": true,
            "valid": {},
            "title": {
                "en": "Width",
                "ko": "Width"
            },
            "help": {
                "en": "Result width.",
                "ko": "결과 너비."
            }
        },
        {
            "rule": "read_and_write",
            "name": "height",
            "default_value": 0,
            "type": "int",
            "required": true,
            "valid": {},
            "title": {
                "en": "Height",
                "ko": "Height"
            },
            "help": {
                "en": "Result height.",
                "ko": "결과 높이."
            }
        },
        {
            "rule": "read_and_write",
            "name": "fx",
            "default_value": 0.0,
            "type": "float",
            "required": true,
            "valid": {},
            "title": {
                "en": "fx",
                "ko": "fx"
            },
            "help": {
                "en": "Scale factor along the horizontal axis.",
                "ko": "가로 축에 따른 배율."
            }
        },
        {
            "rule": "read_and_write",
            "name": "fy",
            "default_value": 0.0,
            "type": "float",
            "required": true,
            "valid": {},
            "title": {
                "en": "fy",
                "ko": "fy"
            },
            "help": {
                "en": "Scale factor along the vertical axis.",
                "ko": "수직 축에 따른 배율."
            }
        },
        {
            "rule": "read_and_write",
            "name": "interpolation",
            "default_value": "linear",
            "type": "str",
            "required": true,
            "valid": {
                "list": "nearest;linear;area;cubic;lanczos4"
            },
            "title": {
                "en": "Interpolation",
                "ko": "Interpolation"
            },
            "help": {
                "en": "Interpolation method.",
                "ko": "보간 방법."
            }
        }
    ]
}
