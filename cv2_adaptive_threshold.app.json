        "environment": {
            "type": "pyenv",
            "name": "cv2"
        },
        "titles": {
            "en": "cv2.adaptiveThreshold",
            "ko": "cv2.adaptiveThreshold"
        },
        "descriptions": {
            "en": "cv2.adaptiveThreshold",
            "ko": "cv2.adaptiveThreshold"
        },
        "documentation_mime": "text",
        "documentations": {},
        "meta": {}
    },
    "controls": {
        "input": "source",
        "output": "result"
    },
    "props": [
        {
            "rule": "read_and_write",
            "name": "max_value",
            "default_value": 255,
            "type": "int",
            "required": true,
            "valid": {},
            "title": {
                "en": "maxValue",
                "ko": "maxValue"
            },
            "help": {
                "en": "maxValue",
                "ko": "maxValue"
            }
        },
        {
            "rule": "read_and_write",
            "name": "block_size",
            "default_value": 3,
            "type": "int",
            "required": true,
            "valid": {},
            "title": {
                "en": "blockSize",
                "ko": "blockSize"
            },
            "help": {
                "en": "blockSize",
                "ko": "blockSize."
            }
        },
        {
            "rule": "read_and_write",
            "name": "c",
            "default_value": 5,
            "type": "int",
            "required": true,
            "valid": {},
            "title": {
                "en": "c",
                "ko": "c"
            },
            "help": {
                "en": "c",
                "ko": "c"
            }
        },
        {
            "rule": "read_and_write",
            "name": "adaptive_thresh",
            "default_value": "gaussian_c",
            "type": "str",
            "required": true,
            "valid": {"list" : "gaussian_c;mean_c"},
            "title": {
                "en": "adaptiveThresh",
                "ko": "adaptiveThresh"
            },
            "help": {
                "en": "adaptiveThresh",
                "ko": "adaptiveThresh"
            }
        },
        {
            "rule": "read_and_write",
            "name": "thresh",
            "default_value": "binary",
            "type": "str",
            "required": true,
            "valid": {
                "list": "binary;binary_inv;mask;otsu;tozero;tozero_inv;triangle;trunc"
            },
            "title": {
                "en": "thresh",
                "ko": "thresh"
            },
            "help": {
                "en": "thresh",
                "ko": "thresh"
            }
        }
